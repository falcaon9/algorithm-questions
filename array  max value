#include<stdio.h>
#include<stdlib.h>


int main (){
	

int list[5]={4,4,2,8,1};  // max value = 8

int maxvalue;

int contvalue;

int sayac =0;

maxvalue=list[0];

while(sayac<=5){
	
	contvalue=list[sayac];
	
	if(maxvalue>contvalue){
		maxvalue=maxvalue;
	}else
	{
	     maxvalue=contvalue;
	}
	sayac++;
}
	printf(" the list max value %d",maxvalue);
	
	
	return 0;
}
