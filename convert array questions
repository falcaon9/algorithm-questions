#include<stdio.h>
#include<stdlib.h>

 int main() {

/**
     Write a C program to read an array of length 6, change the first element by the last, the second element by the fifth and the third element by the fourth. Print the elements of the modified array.
    Test Data:
    Input the 5 members of the array:   Expected Output:
     
    15
    20
    25
    30
    35
     Expected Output:
     array_n[0] = 35
     array_n[1] = 30
     array_n[2] = 25
     array_n[3] = 20
     array_n[4] = 15
     **/
    
    int list1[]={15,20,25,30,35};
    
    int convertlist[]={};
    
    int i;
    int a=0;
    
    for (i=4; i>=0; i--) 
    {
        
        
        convertlist[a]=list1[i];
        
        printf("array_n[%d] = %d",a,convertlist[a]);
        
        printf("\n");
         
        a++;
        
    }
    
    printf("\n");
    
    int c;
    
    for (c=0; c<=4; c++) {
        printf("  %d  ",convertlist[c]);
      }
     
}
    
    
    
    
    
    
    
